  weapon_type                                                    Int?
  last_login                                                     Int?
  savedata                                                       Bytes?
  decomyset                                                      Bytes?
  hunternavi                                                     Bytes?
  otomoairou                                                     Bytes?
  partner                                                        Bytes?
  platebox                                                       Bytes?
  platedata                                                      Bytes?
