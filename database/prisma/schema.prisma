  updated_at                                                     DateTime             @default(now())
  cafe_reset                                                     DateTime?            @db.Timestamptz(6)
  bonus_quests                                                   Int                  @default(0)
  daily_quests                                                   Int                  @default(0)
  promo_points                                                   Int                  @default(0)
  rasta_id                                                       Int?
  pact_id                                                        Int?
