/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model quest_records {
  record_id    String             @db.Uuid
  quest_id     Int
  period       QuestPeriod
  season       QuestSeason
  user_id      Int
  character_id Int
  party_size   Int                @db.SmallInt
  weapon_type  Int
  guild_id     Int?
  status       QuestRecordStatus?
  frames       Int
  time         Int
  started_at   DateTime
  finished_at  DateTime           @default(now())
  created_at   DateTime           @default(now())
  updated_at   DateTime           @default(now())

  @@index([character_id], map: "idx_quest_records_character_id")
  @@index([quest_id], map: "idx_quest_records_quest_id")
  @@index([quest_id, record_id], map: "idx_quest_records_quest_id_record_id")
  @@index([record_id], map: "idx_quest_records_record_id")
  @@index([user_id], map: "idx_quest_records_user_id")
  @@ignore
}

enum ConnectionProviders {
  DISCORD
}

enum QuestPeriod {
  DAY
  NIGHT
}

enum QuestSeason {
  WARM
  COLD
  BREED
}

enum UserLang {
  EN
  JP
}

enum event_type {
  festa
  diva
  vs
  mezfes
}

enum festival_colour {
  none
  red
  blue
}

enum guild_application_type {
  applied
  invited
}

enum logkind {
  NEW_USER
  NEW_CHAR
  USER_LOGIN
  CHAR_LOGIN
  ROAD_CHANGES
}

enum prize_type {
  personal
  guild
}

enum QuestRecordStatus {
  COMPLETED
  FAILED
  RETURNED
}
